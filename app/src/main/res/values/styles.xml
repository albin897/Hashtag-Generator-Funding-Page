<resources xmlns:tools="http://schemas.android.com/tools">




    <style name="signikanegative_medium" tools:ignore="UnusedResources">
        <item name="android:fontFamily">@font/signikanegative_medium</item>
    </style>


    <style name="MyAppBarLayout" parent="Widget.Material3.Toolbar">
        <item name="fontFamily">@font/signikanegative_medium</item>
